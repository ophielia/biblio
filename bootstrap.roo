entity jpa --class ~.catalog.db.FoundDetailsDao --testAutomatically --activeRecord false
repository jpa --interface ~.catalog.db.FoundDetailsRepository --entity ~.catalog.db.FoundDetailsDao

entity jpa --class ~.catalog.db.BookToSubjectDao --testAutomatically --activeRecord false
repository jpa --interface ~.catalog.db.BookToSubjectRepository --entity ~.catalog.db.BookToSubjectDao

entity jpa --class ~.catalog.db.SubjectDao --testAutomatically --activeRecord false
